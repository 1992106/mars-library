{
  "name": "mars-library",
  "version": "0.0.6",
  "main": "dist/mars-library.cjs.js",
  "module": "dist/mars-library.es.js",
  "unpkg": "dist/mars-library.umd.js",
  "browser": "dist/mars-library.umd.js",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/mars-library.es.js",
      "require": "./dist/mars-library.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "build:lib": "vite build --mode lib",
    "serve": "vite preview",
    "lint": "npm run lint:es & npm run lint:style",
    "lint:es": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint:style": "stylelint src/**/*.{css,scss,sass,vue} --fix"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.1.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "vue": "3.0.10",
    "vue-router": "^4.0.0-rc.6",
    "vuex": "^4.0.0-rc.2",
    "vxe-table": "^4.0.18"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.11",
    "commitlint": "^12.1.4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.2.4",
    "vite": "2.3.1",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-mock": "^2.5.0",
    "vite-plugin-style-import": "^0.9.2",
    "vxe-table-plugin-antd": "^3.0.3",
    "xe-ajax": "^4.0.5",
    "xe-utils": "^3.2.1"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.1.6",
    "lodash": "^4.17.21",
    "vue": "3.0.10",
    "vxe-table": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1992106/mars-library.git"
  },
  "homepage": "https://github.com/1992106/mars-library",
  "keywords": [
    "vue3",
    "mars library"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1992106/mars-library/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": [
      "prettier --write",
      "stylelint --fix",
      "eslint --fix",
      "git add"
    ]
  }
}
