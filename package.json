{
  "name": "mars-library",
  "version": "0.3.7",
  "main": "dist/mars-library.umd.js",
  "module": "dist/mars-library.es.js",
  "unpkg": "dist/mars-library.umd.js",
  "browser": "dist/mars-library.umd.js",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/mars-library.es.js",
      "require": "./dist/mars-library.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --force --host 0.0.0.0",
    "build": "vite build",
    "build:lib": "vite build --mode lib",
    "serve": "vite preview",
    "lint": "npm run lint:eslint & npm run lint:stylelint",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,sass,less,vue}\" --fix",
    "prettier": "prettier --write .",
    "cz": "git cz",
    "reinstall": "rimraf yarn.lock && rimraf package-lock.json && rimraf node_modules && yarn"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.21.4",
    "crypto-js": "^4.1.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue": "^3.2.20",
    "vue-draggable-next": "^2.1.1",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vxe-table": "^4.0.30",
    "xe-ajax": "^4.0.5",
    "xe-utils": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.31",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-components": "^0.13.3",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vue-eslint-parser": "^8.3.0",
    "vxe-table-plugin-antd": "^3.0.5"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.8",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.20",
    "vue-draggable-next": "^2.1.1",
    "vxe-table": "^4.0.30"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1992106/mars-library.git"
  },
  "homepage": "https://github.com/1992106/mars-library",
  "keywords": [
    "vue3",
    "vite",
    "vite-vue",
    "mars library"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1992106/mars-library/issues"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss,sass,less,vue}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
